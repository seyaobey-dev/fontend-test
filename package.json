{
  "name": "query-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "dev:server": "nodemon --watch src/server --exec \"npx tsc -p tsconfig.node.json && node dist/server/index.js\""
  },
  "dependencies": {
    "axios": "1.7.7",
    "body-parser": "1.20.3",
    "concurrently": "9.0.1",
    "express": "4.21.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "9.9.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/file-saver": "2.0.7",
    "@types/node": "22.6.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "eslint": "9.9.0",
    "eslint-plugin-react-hooks": "5.1.0-rc.0",
    "eslint-plugin-react-refresh": "0.4.9",
    "globals": "15.9.0",
    "jsdom": "25.0.1",
    "nodemon": "3.1.7",
    "typescript": "5.6.2",
    "typescript-eslint": "8.0.1",
    "vite": "5.4.1",
    "vitest": "2.1.1"
  }
}
